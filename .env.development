# Development Environment Configuration
AZDO_ORGANIZATIONURL=https://dev.azure.com/your-dev-organization
AZDO_PERSONALACCESSTOKEN=your-dev-pat-token

# Development Application Settings
ASPNETCORE_ENVIRONMENT=Development
VERSION=dev
TZ=UTC

# Development Security (less strict)
AZURE_KEY_VAULT_URL=
MANAGED_IDENTITY_CLIENT_ID=
API_KEY_HASH=
ALLOWED_IP_RANGES=

# Development Redis
REDIS_PASSWORD=devpassword
REDIS_CONNECTION_STRING=redis-dev:6379

# Development Monitoring
SENTRY_DSN=
GRAFANA_PASSWORD=admin

# Development Rate Limiting (more lenient)
RATE_LIMIT_REQUESTS_PER_MINUTE=120
RATE_LIMIT_REQUESTS_PER_HOUR=5000

# Development Logging
LOG_LEVEL=Debug
ENABLE_STRUCTURED_LOGGING=true
ENABLE_SENSITIVE_DATA_FILTERING=false

# Development Health Checks
HEALTH_CHECK_TIMEOUT_SECONDS=10
MEMORY_THRESHOLD_PERCENT=95

# Development Features
ENABLE_DEVELOPMENT_FEATURES=true
ENABLE_DEBUG_ENDPOINTS=true